{
    "name": "bjuszczyk/whatsapp-php-client",
    "description": "WhatsApp API PHP Client Library",
    "type": "library",
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Juszczyk\\WhatsApp\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Bartosz Juszczyk",
            "email": "b.juszczyk@bjuszczyk.pl"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
